{
    "name": "so-ali/dcore-installer",
    "description": "Devingo wordpress template core installer.",
    "require": {
        "php": "^7.3|^8.0",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Devingo\\Installer\\Console\\": "includes/"
        }
    },
    "bin": [
        "bin/dcore"
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}