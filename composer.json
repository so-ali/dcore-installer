{
    "name": "so-ali/dcore-installer",
    "description": "Devingo wordpress template core installer.",
    "keywords": ["devingo","dcore","devingo-core"],
    "license": "MIT",
    "authors": [
        {
            "name": "So Ali",
            "email": "ir.alisoleymani@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "symfony/console": "^5.3",
        "symfony/filesystem": "^5.3",
        "symfony/http-client": "^5.4",
        "symfony/process": "^5.3",
        "ext-zip": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "bin": [
        "bin/dcore"
    ],
    "autoload": {
        "psr-4": {
            "Devingo\\Installer\\Console\\": "includes/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
